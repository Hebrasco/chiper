	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#7f8c98"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#fe8170"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword",
				"variable.language.this",
				"variable.language.super",
				"keyword.other.typedef",
				"constant.language"
			],
			"settings": {
				"foreground": "#fe7ab2"
			}
		},
		{
			"name": "ClassDeclName",
			"scope": [
				"entity.name.type.class",
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "Interface",
			"scope": [
				"entity.name.type.interface"
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "InstanceProperties",
			"scope": [
				"variable.other.property",
				"variable.other.object.property",
				"variable.object.property"
			],
			"settings": {
				"foreground": "#b181ec"
			}
		},
		// {
		// 	"name": "InstanceProperties",
		// 	"scope": [
		// 		"variable.object.property"
		// 	],
		// 	"settings": {
		// 		"foreground": "#65b5a2"
		// 	}
		// },
		{
			"name": "OtherDeclarations",
			"scope": [
				"entity.name.function",
				"entity.name.function.method"
			],
			"settings": {
				"foreground": "#4db1cb"
			}
		},
		{
			"name": "ModuleNames",
			"scope": [
				"entity.name.type.module",
				"entity.other.inherited-class",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#d9baff"
			}
		},
		{
			"name": "Markup bold",
			"scope": [
				"markup.bold",
				"markdown.bold",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "Markup heading",
			"scope":  [
				"markup.heading",
				"markdown.heading",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "Markup italic",
			"scope":  [
				"markup.italic",
				"markdown.italic",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d9baff"
			}
		},
		{
			"name": "Markup quote",
			"scope":  [
				"markup.quote",
				"markdown.quote",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#d9baff"
			}
		},
		{
			"name": "TypeAnnotation",
			"scope": [
				"meta.type.annotation"
			],
			"settings": {
				"foreground": "#d9baff"
			}
		},
		{
			"name": "tag",
			"scope": [
				"punctuation.definition.tag",
				"entity.name.tag",
				"punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "#fe7ab2"

			}
		},
		{
			"name": "JSSeparator",
			"scope": [
				"punctuation.separator.key-value.js"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Attribute",
			"scope": [
				"meta.attribute",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#65b5a2"//"#b181ec"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#d8c87c"
			}
		},
		{
			"name": "Operator",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Parameter",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "ClassType",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#d9baff"
			}
		},
		{
			"name": "Assignment",
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "GQLFragmentName",
			"scope": [
				"entity.name.fragment.graphql",
				"variable.fragment.graphql"
			],
			"settings": {
				"foreground": "#6bdfff"
			}
		},
		{
			"name": "QuasiElement",
			"scope": [
				"punctuation.quasi.element"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.other.readwrite",
				"variable.other.constant",
				"variable.other.object",
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Brace",
			"scope": [
				"meta.brace"
			],
			"settings": {
				"foreground": "#ffffffd9"
			}
		},
		{
			"name": "Arithmetic",
			"scope": [
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#b281eb"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}